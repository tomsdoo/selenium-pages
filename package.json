{
  "name": "selenium-pages",
  "version": "1.8.0",
  "description": "It's an extensible page based class library for selenium. TypeScript is supported.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "tsc --project tsconfig.esm.json && tsc --project tsconfig.cjs.json",
    "serve:dev": "node dev_server",
    "test": "npm run build && TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha -r ts-node/register \"test/**/*.ts\" --timeout 20000"
  },
  "keywords": [
    "javascript",
    "selenium",
    "typescript"
  ],
  "author": "tom",
  "license": "MIT",
  "dependencies": {
    "selenium-webdriver": "^4.1.0"
  },
  "devDependencies": {
    "@types/koa": "^2.13.4",
    "@types/koa-router": "^7.4.4",
    "@types/koa-static-server": "^1.5.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.4",
    "@types/selenium-webdriver": "^4.0.16",
    "koa": "^2.13.4",
    "koa-router": "^10.1.1",
    "koa-static-server": "^1.5.2",
    "mocha": "^9.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
